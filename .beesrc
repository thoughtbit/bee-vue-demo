{
  "use": "vue",
  "style": ["css", "less"],
  "publicPath": "/",
  "extraBabelPlugins": [
    "transform-vue-jsx"
  ],
  "disableCSSModules": false,
  "imgCompress": true,
  "autoprefixer": null,
  "externals": [],
  "multipage": true,
  "define": null,
  "proxy": {
    "/api": {
      "target": "http://jsonplaceholder.typicode.com/",
      "changeOrigin": true,
      "pathRewrite": { "^/api" : "" }
    }
  },
  "env": {
    "development": {
      "cssSourceMap": false
    },
    "production": {
      "cssSourceMap": true,
      "analyze": true
    }
  },
  "dllPlugin": {
    "name": "bees",
    "exclude": [
      "babel-runtime"
    ],
    "include": [
      "vue/dist/vue.js"
    ]
  }
}